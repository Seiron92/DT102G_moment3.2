@model moment3._2.Models.Cd

@{
    ViewData["Title"] = "Detaljer";
      var num = 0;
}

<h1>Detaljer</h1>

<div>
    <h4>Cd</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
        Namn
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Name)
        </dd>
        <dt class="col-sm-2">
         Releasedatum
        </dt>
        <dd class="col-sm-10">

            @ViewData["Date"]


        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Avalable)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Avalable)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Artist)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Artist.Name)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Tracks)
        </dt>
        <dd class="col-sm-10">
           
      @foreach(var test in Model.Tracks)
      {
        
          num++;
            <p>@num.  @test.Name</p>
      }



        </dd>
    </dl>
</div>
<div>
    <a asp-action="Edit" asp-route-id="@Model.CdId">Ändra</a> |
    <a asp-action="Index">Back to List</a>
</div>
